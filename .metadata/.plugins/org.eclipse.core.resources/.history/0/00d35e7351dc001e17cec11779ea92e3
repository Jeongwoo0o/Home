package e_method;

public class Ex09_복습2 {
	public static void main(String[] args) {

		input();
		output();

	}
	
	/**
	 * 	역할 : 두 정수와 알파벳 문자 하나를 입력받음
	 * 	[예] 두 정수와 알파벳 문자 하나를 입력 : 
	 * 		3 4 F
	 */
	static void input() {
		makeSquare();
	}

	/**
	 * 	역할 : input()함수에서 입력받은 첫번째 정수만큼의 행과
	 * 			두번째 정수만큼의 열의 배열을 만들어서
	 * 			입력받은 문자로 시작하는 배열값을 저장합니다
	 * 
	 * 	[예]
	 * 		F	G	H	I
	 * 		J	K	L	M
	 * 		N	O	P	Q
	 */
	static void makeSquare() {
		
	}
	
	static void output() {
		
	}
}













































