package c_control;

/*
 * 		break		: 블럭을 벗어나는 문장
 * 		continue	: 블럭의 끝으로 가는 문장
 */

public class Ex12_Break_Continue {
	public static void main(String[] args) {

		for(int i = 0; i < 2; i++) {
			for(int j = 0; j < 3; j++) {
				if( j == 2) break;
				System.out.println("<" + i + ", " + j + ">");
			}	//	end for(j)
			System.out.println("데이터");
		}	//	end for(i)
		
	}
}



































