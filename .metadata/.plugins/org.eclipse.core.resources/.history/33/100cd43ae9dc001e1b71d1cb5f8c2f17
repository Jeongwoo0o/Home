package e_method;

public class Ex06_main인자 {
	
	// 함수명(메소드명)	: main
	// 리턴형			: void
	//					-반환하는 리턴값이 없음
	// 매개변수		: String[]
	
	public static void main(String[] args) {
		System.out.println(args[3]);
		for(int i = 0; i < args.length; i++) {
			System.out.println(args[i]);
		}

	}
	
	/*
	 * 	> javac Ex06_main인자.java
	 * 
	 * 	> java Ex06_main인자 문자열
	 */
	
}






































