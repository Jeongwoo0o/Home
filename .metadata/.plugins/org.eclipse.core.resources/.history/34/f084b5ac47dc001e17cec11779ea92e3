package e_method;

/*
 * 	Call by Reference
 * 
 * 		-메소드의 인자가 참조형인 경우
 */

public class Ex05_CallByRef {
	public static void main(String[] args) {

		StringBuffer a = new StringBuffer("안녕");
		StringBuffer b = new StringBuffer("하이");
		
		System.out.println("A = " + a + " B = " + b);

	}
}











































