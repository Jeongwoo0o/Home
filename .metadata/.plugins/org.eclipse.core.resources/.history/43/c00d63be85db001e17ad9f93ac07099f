package d_array;

public class 복습 {
	public static void main(String[] args) {

		int [ ] arr = { 66, 55, 44, 33, 22, 11 };
        int  sum = 0;

        /*  배열의 각각의 값을 더하는 코드 */ 
        for(int i = 0; i < arr.length; i++) {
        	sum += arr[i];
        }

        System.out.println( "sum = " + sum );

	}
}

































