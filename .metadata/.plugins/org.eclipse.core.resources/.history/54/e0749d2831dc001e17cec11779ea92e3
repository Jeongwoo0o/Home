
public class max {
	public static void main(String[] args) {

		int a[][]=new  int[][]{{3,-5, 12 }, {-2, 11, 2, -7}, {21, -21, -35, -93, -11}, {9, 14, 39, -98}};
		int max = 0;
		
		for(int i = 0; i < a.length; i++) {				// 4
			for(int j = 0; j < a[i].length; j++) {		// 5
			}
			a[0][i] = max;
		}	//end for(i)

	}
}
