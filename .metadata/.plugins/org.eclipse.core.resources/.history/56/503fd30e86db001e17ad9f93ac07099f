package d_array;

public class 복습 {
	public static void main(String[] args) {

//		int [ ] arr = { 66, 55, 44, 33, 22, 11 };
//		int  sum = 0;
//
//		/*  배열의 각각의 값을 더하는 코드 */ 
//		for(int i = 0; i < arr.length; i++) {
//			sum += arr[i];
//		}
//
//		System.out.println( "sum = " + sum );

		int  [] [] arr = { 	{ 90, 90, 90, 90, 90 },
				{ 80, 80, 80, 80, 80 },
				{ 70, 70, 70, 70, 70 },
				{ 60, 60, 60, 60, 60 } };

		int  sum = 0;

		/*  배열의 각각의 값을 더하는 코드 */ 
		for(int i = 0; i < arr.length; i++) {
			for(int j = 0; j < arr[i].length; j++) {
				sum += arr[i][j];
			}	//end for(j)
		}	//end for(i)

		System.out.println( "sum = " + sum );

	}
}

































