package c_control;

public class Ex09_while개념 {
	public static void main(String[] args) {

		int sum = 0;
		
		int i = 1;
		
//		for(; i <= 10; i++) {
//			sum += i;
//		}	//	end for(i)
		
		while(i <= 10) {
			sum += 1;
			i++;
		}
		
		System.out.println("합 : " + sum);

	}
}















































