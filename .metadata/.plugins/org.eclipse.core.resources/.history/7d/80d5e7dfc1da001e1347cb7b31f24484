		Scanner in = new Scanner(System.in);
		System.out.print("숫자 입력 : ");
		int n = in.nextInt();
		
		char a = 'A';
		for(int i = 0; i < n; i++) {
			for(int j = n-i; j > 0; j--) {
				System.out.print(a + " ");
				a++;
			}	//	end for(j)
			
			System.out.println();
		}	//	end for(i)
		
