package c_control;

public class Ex10_dowhile개념 {
	public static void main(String[] args) {

		int i = 0;
		int sum = 0;
		
		for(i = 1; i <= 10; i++) {
			sum += i;
		}	//	end for(i)
		
		System.out.println(sum);
		
		while(i < 10) {
			sum += i;
			i++;
		}	//	end while(i)

	}
}












































