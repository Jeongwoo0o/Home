package c_control;

/*
 * 		[참고]
 * 			
 * 			 9 	: 숫자
 * 			'9' : 문자
 * 			"9" : 문자열
 */

public class 연습_나이구하기 {
	public static void main(String[] args) {

		String id = "901212-1234567";
		String nai_str = id.substring(0, 2);			//	90 가져오기
		
		//	"90"	-> 90
		int nai = Integer.parseInt(nai_str);			//	String	-> int 형 변환
		
		System.out.println(nai - 1);

	}
}









































