package e_method;
import java.util.Scanner;

public class Ex09_복습 {
	public static void main(String[] args) {

		func();

	}
	
	// 역할 : 콘솔창에서 점수를 입력받기
	static void func() {
		
		Scanner in = new Scanner(System.in);
		System.out.print("점수 : ");
		int score = in.nextInt();
		System.out.println("당신의 점수 : " + score);
		
	}
	
}




































