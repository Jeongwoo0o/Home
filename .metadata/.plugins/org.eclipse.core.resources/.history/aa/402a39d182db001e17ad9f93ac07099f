package d_array;

public class Ex05_2차원개념 {
	public static void main(String[] args) {

		int[][] data = new int[3][2];
		
		data[0][0] = 1000;
		data[1][1] = -200;
		
		// 출력
		for(int i = 0; i < 3; i++) {
			for(int j = 0; j < 2; j++) {
				System.out.print(data[i][j] + " ");
			}	//end for(j)
			System.out.println();
		}	//end for(i)

	}
}












































