package d_constructor2;

public class GradeExpr {
	
	int[] jumsu;
	
	GradeExpr(){}
	
	GradeExpr(int[] jumsu) {
		this.jumsu = jumsu;
	}
	
	double getAverage() {
		int total = getTotal();
		return total / jumsu.length;
	}
	
	int getTotal() {
		int total = 0;
		for(int i = 0; i < jumsu.length; i++) {
			total += jumsu[i];
		}
		return total;
	}
	
	int getGoodScore() {
		return 0;
	}
	
	int getBadScore() {
		return 0;
	}
}
