package e_method;

public class Ex02_반환2 {
	public static void main(String[] args) {

		int[] data = add();
		
		for(int i = 0; i < data.length; i++) {
			System.out.println(data[i]);
			int sum = data[i]+data[i+1];
		}
		System.out.println(sum);
		
	}

	static int[] add() {
		
		int a = 10, b = 20;
		int[] data = {a, b};
		
		return data;
	}
	
}
