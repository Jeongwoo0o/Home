package d_array;

public class Ex02_최대값찾기 {
	public static void main(String[] args) {

		int [] su = {99,4,20,88,57,29,101};
		int max = su[0];
		
		for(int i = 0; i < su.length; i++) {
			if(max < su[i]) {
				max = su[i];
			}	//	end if
		}	//	end for(i)
		System.out.println("MAX : " + max);
		
	}
}


































