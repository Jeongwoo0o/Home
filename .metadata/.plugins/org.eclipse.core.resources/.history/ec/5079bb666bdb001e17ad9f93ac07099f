package d_array;
import java.util.Scanner;

public class Ex02_최대값찾기 {
	public static void main(String[] args) {

		Scanner in = new Scanner(System.in);
		System.out.print("정수 입력(띄어쓰기로 구분) : ");
		int num = in.nextInt();
		
		int [] su = new int[num];
		int max = su[0];
		
		for(int i = 0; i < su.length; i++) {
			if(max < su[i]) {
				max = su[i];
			}	//	end if
		}	//	end for(i)
		System.out.println("MAX : " + max);
		
	}
}


































